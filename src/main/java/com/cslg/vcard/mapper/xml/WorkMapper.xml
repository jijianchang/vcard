<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cslg.vcard.mapper.WorkMapper">

    <select id="queryWork" resultType="com.cslg.vcard.entity.Work">
         select *
        from work
        where eid=#{eid}
    </select>
</mapper>
